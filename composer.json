{
    "name": "noondaysun/dweetio",
    "type": "package",
    "description": "Send and recieve messages from https://dweet.io.",
    "homepage": "https://github.com/noondaysun/dweetio",
    "license": "GNU GPL v2.0",
    "autoload": {
        "psr-4": {
            "Noondaysun\\Dweetio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noondaysun\\Dweetio\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.4",
        "monolog/monolog": "~1.6",
        "nunomaduro/phpinsights": "^1.11",
        "pds/skeleton": "^1.0",
        "php": "^7.2",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "mayflower/php-codebrowser": "^2.2",
        "pdepend/pdepend" : "^2.5",
        "phing/phing": "^2.16",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd" : "^2.7",
        "phpunit/phpunit": "^8.4",
		"squizlabs/php_codesniffer": "^3.5",
        "sebastian/phpcpd": "*"
    },
    "scripts": {
        "build": "phing build",
        "insights": "phpinsights analyse -n -- ./src"
    },
    "version":"0.2.0"
}
